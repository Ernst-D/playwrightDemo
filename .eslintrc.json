{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": ["eslint:recommended","plugin:jest-playwright/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }
}